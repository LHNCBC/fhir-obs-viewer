{
  "name": "fhir-obs-viewer",
  "version": "1.13.0",
  "description": "A viewer for Observation resources on a FHIR server",
  "main": "obs-viewer.js",
  "scripts": {
    "update-webdriver": "node node_modules/protractor/bin/webdriver-manager update --versions.chrome $(google-chrome --version |awk '{print $NF}')",
    "test:unit": "jest",
    "test:e2e": "webpack-dev-server --config webpack.development.js & sleep 5; protractor test/e2e/protractor.conf.js; s=$?; kill %1; exit $s",
    "lint": "eslint source",
    "lint-fix": "eslint --fix source && prettier --write source/",
    "pretest": "npm run update-webdriver",
    "test": "npm run lint && npm run test:unit && npm run test:e2e",
    "build": "webpack --config webpack.production.js",
    "start": "webpack-dev-server --open --config webpack.development.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lhncbc/fhir-obs-viewer.git"
  },
  "keywords": [
    "FHIR"
  ],
  "author": "plynch@mail.nih.gov",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/preset-env": "^7.6.0",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.1.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jasmine": "^4.1.1",
    "extract-loader": "^3.1.0",
    "file-loader": "^4.1.0",
    "file-saver": "^2.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jasmine-spec-reporter": "^4.2.1",
    "jest": "^24.1.0",
    "loader-utils": "^1.4.0",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.24.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "prettier": "2.0.5",
    "protractor": "^7.0.0",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  }
}
